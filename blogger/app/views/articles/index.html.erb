<h1>All Articles</h1>

<ul id="articles">
	<% @articles.each do |article| %>
		<li>
			<%= link_to article.title, article,
			class: "article_title" %>
		</li>
	<% end %>
</ul>

<br />
<br />

<%= link_to "View All Tags", tags_path %>

<%= link_to "Create New Article", new_article_path, class: "new_article" %>
